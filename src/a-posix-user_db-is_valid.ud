(section
  (ref "posix_user_db_is_valid")
  (title "POSIX.User_DB.Is_Valid")
  (para function_desc "Database entry is valid?")
  (subsection
    (title "Synopsis")
    (para-verbatim function_synopsis
"function Is_Valid (Database_Entry : in Database_Entry_t) return Boolean;
"))
  (subsection
    (title "Description")
    (para
      "The " (item ada_function "Is_Valid") " function retrieves
      returns " (item constant "True") " if the database entry "
      (item parameter "Database_Entry") " is valid.")
    (para
      "This function is used by " (item package "SPARK") " to check
      pre/postconditions. It should not be necessary to use this
      function explicitly.")))
